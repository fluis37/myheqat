@EndUserText.label: 'access control'
@MappingRole: true
define role Z08_R_TRAVEL {
    grant 
        select
            on
                Z08_R_TRAVEL
                    where
                        (AgencyId) = aspect pfcg_auth(
                                /LRN/AGCY,
                                /LRN/AGCY,
                                ACTVT = '03' );                         
}