@EndUserText.label: 'Autorisation base'
@MappingRole: true
define role Z07_R_TRAVEL {
    grant 
        select
            on
                Z07_R_TRAVEL
                    where
                        (AgencyId) = aspect pfcg_auth(/LRN/AGCY, /LRN/AGCY, ACTVT = '03');
                        
}